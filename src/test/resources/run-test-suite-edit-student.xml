<?xml version = "1.0" encoding = "UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name = "Test suite edit student">
    <listeners>
        <listener class-name="utils.listeners.TestListener"/>
    </listeners>
    <test name = "Edit student success with admin">
        <groups>
            <run>
                <include name = "admin" />
            </run>
        </groups>
        <classes>
            <class name = "testcase.TestCaseEditStudent" />
        </classes>
    </test>
    <test name = "Edit student success with teacher">
        <groups>
            <run>
                <include name = "teacher" />
            </run>
        </groups>
        <classes>
            <class name = "testcase.TestCaseEditStudent" />
        </classes>
    </test>


</suite>